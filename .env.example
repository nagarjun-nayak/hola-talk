# Database\nDATABASE_URL=mysql://username:password@hostname:port/database_name

# NextAuth
NEXTAUTH_SECRET=your_nextauth_secret
NEXTAUTH_URL=http://localhost:3000

# Google OAuth
GOOGLE_CLIENT_ID=your_google_client_id
GOOGLE_CLIENT_SECRET=your_google_client_secret

# Pusher
PUSHER_APP_ID=your_pusher_app_id
PUSHER_KEY=your_pusher_key
PUSHER_SECRET=your_pusher_secret
PUSHER_CLUSTER=your_pusher_cluster
NEXT_PUBLIC_PUSHER_KEY=your_pusher_key
NEXT_PUBLIC_PUSHER_CLUSTER=your_pusher_cluster

# LiveKit
LIVEKIT_API_KEY=your_livekit_api_key
LIVEKIT_API_SECRET=your_livekit_api_secret
NEXT_PUBLIC_LIVEKIT_API_HOST=wss://your-livekit-instance.com

# OneAI Summarizer
ONEAI_API_KEY=your_oneai_api_key

# OpenAI (if used)
OPEN_API_SECRET=your_openai_api_key

# MarianMT Configuration (replacing Google Translate)
MARIAN_MT_API_ENDPOINT=https://api.example.com/translate
MARIAN_MT_API_KEY=your_marian_mt_api_key
MARIAN_MT_MAX_LENGTH=512
MARIAN_MT_BATCH_SIZE=8
MARIAN_MT_DEFAULT_MODEL=opus-mt

# Client-side MarianMT Configuration
NEXT_PUBLIC_MARIAN_MT_API_ENDPOINT=https://api.example.com/translate
NEXT_PUBLIC_MARIAN_MT_API_KEY=your_marian_mt_api_key
NEXT_PUBLIC_MARIAN_MT_DEFAULT_MODEL=opus-mt
